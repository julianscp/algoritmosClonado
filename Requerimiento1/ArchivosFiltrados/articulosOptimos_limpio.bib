  publisher = {Elsevier BV},
  volume = {473},
  pages = {116817},
  doi = {10.1016/j.cam.2025.116817},
  url = {https://doi.org/10.1016/j.cam.2025.116817},
  abstract = {In this paper, we present an improved version of the 
                        
                           
