  title = {Retraction: Artistic mathematical analysis of the influence of modern urban architecture and lighting on human visual information transmission},
  journal = {Journal of Computational Methods in Sciences and Engineering},
  year = {2025},
  publisher = {SAGE Publications},
  doi = {10.1177/14727978251366166},
  url = {https://doi.org/10.1177/14727978251366166},
